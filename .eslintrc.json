{
  "env": {
    "node": true,
    "es2020": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "off",
    "no-var": "error",
    "prefer-const": "error",
    "curly": "error",
    "eqeqeq": "error",
		"no-unused-vars": "off"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "plugins": ["prettier"],
  "parser": "@typescript-eslint/parser"
}
